{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Projects",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "projects"
    },
    "description": {
      "type": "richtext"
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post"
    },
    "github_url": {
      "type": "string"
    },
    "demo_url": {
      "type": "string"
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Software",
        "Engineering"
      ],
      "required": true,
      "default": "Software"
    },
    "highlighted": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "red",
        "green",
        "blue",
        "yellow"
      ]
    },
    "tools": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tool.tool"
    },
    "excerpt": {
      "type": "text",
      "maxLength": 140
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
